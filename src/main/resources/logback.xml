<configuration>
    <appender name="FILE" class="com.omnine.agent.shaded.ch.qos.logback.core.FileAppender">
        <file>c:/temp/threadbump.log</file>
        <encoder>
            <pattern>%date %level [%thread] %logger{10} [%file:%line] %msg%n</pattern>
        </encoder>
    </appender>

    <root level="debug">
        <appender-ref ref="FILE" />
    </root>
</configuration>